{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "madge": {
      "outputs": [],
      "dependsOn": ["^madge"],
      "inputs": ["./packages/core/**/*.ts", "./packages/core/**/*.ts"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "@midi-structor/electron#make": {
      "dependsOn": ["@midi-structor/ui#build"]
    }
  }
}
