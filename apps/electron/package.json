{
  "name": "@midi-structor/electron",
  "version": "0.0.1",
  "private": true,
  "type": "commonjs",
  "main": ".webpack/main",
  "dependencies": {
    "copyfiles": "^2.4.1",
    "electron-log": "^5.3.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.3.9",
    "tslib": "^2.3.0",
    "typescript": "^5.7.3"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "predist": "copyfiles -u 1 src/assets/**/* ../../dist/apps/electron/",
    "preserve": "tsc",
    "serve": "electron ../../dist/apps/electron/src/index.js",
    "package": "electron-forge package",
    "start": "electron-forge start ",
    "make": "DEBUG=electron-packager electron-forge make",
    "pack:main": "NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config webpack.main.config.js",
    "pack:renderer": "webpack --config webpack.renderer.config.js"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.7.0",
    "@electron-forge/maker-deb": "^7.7.0",
    "@electron-forge/maker-dmg": "^7.7.0",
    "@electron-forge/maker-rpm": "^7.7.0",
    "@electron-forge/maker-squirrel": "^7.7.0",
    "@electron-forge/maker-zip": "^7.7.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.7.0",
    "@electron-forge/plugin-fuses": "^7.7.0",
    "@electron-forge/plugin-local-electron": "^7.7.0",
    "@electron-forge/plugin-webpack": "^7.7.0",
    "@electron/fuses": "^1.8.0",
    "css-loader": "^7.1.2",
    "electron": "^34.2.0",
    "ts-loader": "^9.5.2"
  }
}
